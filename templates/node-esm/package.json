    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --no-watch"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2"
  },
  "lint-staged": {
    "*": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "browserslist": [
    "node 18.14.1"
  ],
  "engines": {
    "node": ">=18.14.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@4.12.0"
}

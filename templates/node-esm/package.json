{
  "name": "@heygrady/node-esm",
  "version": "1.0.9",
  "type": "module",
  "main": "./lib/index.js",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "clean": "rimraf .turbo",
    "coverage": "yarn test --coverage",

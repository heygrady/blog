{
  "name": "@heygrady/jest-preset-ts-node-esm",
  "version": "1.0.6",
  "type": "commonjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./jest-preset.js",
  "files": [
    "__mocks__/**/*",
    "jest-preset.js"
  ],
  "scripts": {
    "format": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@types/jest": "^29",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "jest-config": "^29.6.4",
    "ts-jest": "^29.1.1",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@heygrady/eslint-config": "^1.5.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/eslint": "^8",
    "@types/prettier": "^2",
    "@types/rimraf": "^4",
    "eslint": "^8.48.0",
    "lint-staged": "^14.0.1"
  },
  "lint-staged": {
    "*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node 16.14.0"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@3.2.2"
}

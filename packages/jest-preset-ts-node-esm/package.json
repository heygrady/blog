{
  "name": "@heygrady/jest-preset-ts-node-esm",
  "version": "1.0.9",
  "type": "commonjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./jest-preset.js",
  "files": [
    "__mocks__/**/*",
    "jest-preset.js"
  ],
  "scripts": {
    "format": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@types/jest": "^30.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-config": "^30.2.0",
    "ts-jest": "^29.4.5",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@heygrady/eslint-config": "workspace:^",
    "@types/eslint": "^9.6.1",
    "@types/prettier": "^2.7.3",
    "@types/rimraf": "^4.0.5",
    "eslint": "^9.39.1",
    "lint-staged": "^16.2.7"
  },
  "lint-staged": {
    "*.{md,cjs,mjs,js,jsx,cts,.ts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node 18.14.1"
  ],
  "engines": {
    "node": ">=18.14.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@4.12.0"

{
  "name": "@heygrady/eslint-config",
  "version": "1.5.1",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./eslint.config.mjs",
  "files": [
    "lib/**/*",
    "jest/**/*",
    ".prettierrc.mjs",
    "astro.js",
    "astro-react.js",
    "astro-solid.js",
    "node.js",
    "README.md",
    "eslint.config.js",
    "ts-node-esm.js",
    "tsx-react-esm.js",
    "tsx-solid-esm.js",
    "zx.js"
  ],
  "scripts": {
    "clean": "rimraf .turbo",
    "format": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "@eslint/markdown": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "astro-eslint-parser": "^1.2.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^61.4.1",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-storybook": "^10.1.2",
    "eslint-plugin-testing-library": "^7.13.5",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.5.0",
    "package-directory": "^8.1.0",
    "prettier": "^3.7.3",
    "prettier-config-standard": "^7.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "eslint": "^9.39.1",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "lint-staged": {
    "*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node 18.18.0"
  ],
  "engines": {
    "node": ">=18.14.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@4.12.0"
}

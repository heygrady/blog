{
  "name": "@heygrady/eslint-config",
  "version": "1.3.1",
  "type": "commonjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./ts-node-esm.js",
  "files": [
    "lib/**/*",
    ".prettierrc.js",
    "node.js",
    "README.md",
    "ts-node-esm.js",
    "tsx-react-esm.js",
    "zx.js"
  ],
  "scripts": {
    "clean": "rimraf .turbo",
    "format": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@testing-library/dom": "^9.3.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^5.11.0",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.8.8",
    "prettier-config-standard": "^5.0.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/rimraf": "^4",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1"
  },
  "lint-staged": {
    "*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node 16.14.0"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@3.2.2"
}

{
  "name": "@heygrady/eslint-config",
  "version": "1.5.1",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./eslint.config.mjs",
  "files": [
    "lib/**/*",
    ".prettierrc.mjs",
    "node.js",
    "README.md",
    "eslint.config.js",
    "ts-node-esm.js",
    "tsx-react-esm.js",
    "zx.js"
  ],
  "scripts": {
    "clean": "rimraf .turbo",
    "format": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\" --ignore-pattern \"package.json\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@eslint/markdown": "^7.5.1",
    "@testing-library/dom": "latest",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "latest",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "latest",
    "eslint-plugin-compat": "latest",
    "eslint-plugin-eslint-comments": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-jest-dom": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-n": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-storybook": "latest",
    "eslint-plugin-testing-library": "latest",
    "eslint-plugin-vitest": "latest",
    "package-directory": "^8.1.0",
    "prettier": "latest",
    "prettier-config-standard": "latest",
    "typescript": "latest",
    "vitest": "latest"
  },
  "devDependencies": {
    "@types/rimraf": "^4",
    "lint-staged": "latest",
    "rimraf": "latest"
  },
  "lint-staged": {
    "*.{md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node 18.18.0"
  ],
  "engines": {
    "node": ">=18.18.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@3.2.2"
}

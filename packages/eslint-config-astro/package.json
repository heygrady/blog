{
  "name": "@heygrady/eslint-config-astro",
  "version": "1.5.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "type": "commonjs",
  "license": "MIT",
  "main": "./astro.js",
  "files": [
    "lib/**/*",
    ".prettierrc.js",
    "astro.js",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf .turbo",
    "format": "eslint \"**/*.{astro,md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{astro,md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@heygrady/eslint-config": "^1.4.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-astro": "0.29.0",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vitest": "^0.2.8",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.8.8",
    "prettier-config-standard": "^5.0.0",
    "prettier-plugin-astro": "0.10.0",
    "typescript": "^5.1.3",
    "vitest": "^0.34.3"
  },
  "devDependencies": {
    "@types/rimraf": "^4",
    "lint-staged": "^14.0.1",
    "rimraf": "^5.0.1"
  },
  "lint-staged": {
    "*.{astro,md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node 16.14.0"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@3.2.2"
}

{
  "name": "@heygrady/eslint-config-astro",
  "version": "1.2.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/blog.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "type": "commonjs",
  "license": "MIT",
  "main": "./astro.js",
  "files": [
    "lib/**/*",
    ".prettierrc.js",
    "astro.js",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf .turbo",
    "format": "eslint \"**/*.{astro,md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\" --fix",
    "lint": "eslint \"**/*.{astro,md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}\"",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@heygrady/eslint-config": "^1.2.4",
    "@rushstack/eslint-patch": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.45.0",
    "eslint-plugin-astro": "0.27.2",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.8.8",
    "prettier-config-standard": "^5.0.0",
    "prettier-plugin-astro": "0.10.0",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/rimraf": "^4",
    "lint-staged": "^13.2.3",
    "rimraf": "^5.0.1"
  },
  "lint-staged": {
    "*.{astro,md,cjs,mjs,js,jsx,cts,mts,ts,tsx,json}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "node 16.14.0"
  ],
  "engines": {
    "node": ">=16.14.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "yarn@3.2.2"
}
